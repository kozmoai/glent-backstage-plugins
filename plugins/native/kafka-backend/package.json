{
  "name": "@kozmoai/plugin-kafka-backend",
  "version": "0.3.12-next.1",
  "description": "A Glint backend plugin that integrates towards Kafka",
  "glint": {
    "role": "backend-plugin"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "glint",
    "kafka"
  ],
  "homepage": "https://glint.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/kozmoai/glint",
    "directory": "plugins/kafka-backend"
  },
  "license": "Apache-2.0",
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha.ts",
    "./package.json": "./package.json"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "files": [
    "dist",
    "config.d.ts",
    "alpha"
  ],
  "scripts": {
    "build": "glint-cli package build",
    "clean": "glint-cli package clean",
    "lint": "glint-cli package lint",
    "prepack": "glint-cli package prepack",
    "postpack": "glint-cli package postpack",
    "start": "glint-cli package start",
    "test": "glint-cli package test"
  },
  "dependencies": {
    "@kozmoai/backend-common": "workspace:^",
    "@kozmoai/backend-plugin-api": "workspace:^",
    "@kozmoai/config": "workspace:^",
    "@kozmoai/errors": "workspace:^",
    "@types/express": "^4.17.6",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "kafkajs": "^2.0.0",
    "lodash": "^4.17.21",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@kozmoai/cli": "workspace:^",
    "@types/jest-when": "^3.5.0",
    "@types/lodash": "^4.14.151",
    "jest-when": "^3.1.0",
    "supertest": "^6.1.3"
  },
  "configSchema": "config.d.ts"
}
