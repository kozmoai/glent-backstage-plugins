{
  "name": "@kozmoai/plugin-catalog-common",
  "version": "1.0.22-next.1",
  "description": "Common functionalities for the catalog plugin",
  "glint": {
    "role": "common-library"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "glint"
  ],
  "homepage": "https://glint.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/kozmoai/glint",
    "directory": "plugins/catalog-common"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha.ts",
    "./package.json": "./package.json"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "glint-cli package build",
    "clean": "glint-cli package clean",
    "lint": "glint-cli package lint",
    "prepack": "glint-cli package prepack",
    "postpack": "glint-cli package postpack",
    "test": "glint-cli package test"
  },
  "dependencies": {
    "@kozmoai/catalog-model": "workspace:^",
    "@kozmoai/plugin-permission-common": "workspace:^",
    "@kozmoai/plugin-search-common": "workspace:^"
  },
  "devDependencies": {
    "@kozmoai/cli": "workspace:^"
  }
}
