{
  "name": "@kozmoai/plugin-app-backend",
  "description": "A Glint backend plugin that serves the Glint frontend app",
  "version": "0.3.62-next.1",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "glint": {
    "role": "backend-plugin"
  },
  "homepage": "https://glint.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/kozmoai/glint",
    "directory": "plugins/app-backend"
  },
  "keywords": [
    "glint"
  ],
  "scripts": {
    "start": "glint-cli package start",
    "build": "glint-cli package build",
    "lint": "glint-cli package lint",
    "test": "glint-cli package test",
    "prepack": "glint-cli package prepack",
    "postpack": "glint-cli package postpack",
    "clean": "glint-cli package clean"
  },
  "dependencies": {
    "@kozmoai/backend-common": "workspace:^",
    "@kozmoai/backend-plugin-api": "workspace:^",
    "@kozmoai/config": "workspace:^",
    "@kozmoai/config-loader": "workspace:^",
    "@kozmoai/plugin-app-node": "workspace:^",
    "@kozmoai/types": "workspace:^",
    "@types/express": "^4.17.6",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "fs-extra": "^11.2.0",
    "globby": "^11.0.0",
    "helmet": "^6.0.0",
    "knex": "^3.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.0.0",
    "winston": "^3.2.1",
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "@kozmoai/backend-app-api": "workspace:^",
    "@kozmoai/backend-test-utils": "workspace:^",
    "@kozmoai/cli": "workspace:^",
    "@kozmoai/types": "workspace:^",
    "@types/supertest": "^2.0.8",
    "node-fetch": "^2.6.7",
    "supertest": "^6.1.3"
  },
  "configSchema": "config.d.ts",
  "files": [
    "dist",
    "config.d.ts",
    "migrations/**/*.{js,d.ts}",
    "static"
  ]
}
