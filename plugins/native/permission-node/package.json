{
  "name": "@kozmoai/plugin-permission-node",
  "description": "Common permission and authorization utilities for backend plugins",
  "version": "0.7.25-next.1",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "glint": {
    "role": "node-library"
  },
  "homepage": "https://glint.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/kozmoai/glint",
    "directory": "plugins/permission-node"
  },
  "keywords": [
    "glint",
    "permissions"
  ],
  "scripts": {
    "build": "glint-cli package build",
    "lint": "glint-cli package lint",
    "test": "glint-cli package test",
    "prepack": "glint-cli package prepack",
    "postpack": "glint-cli package postpack",
    "clean": "glint-cli package clean",
    "start": "glint-cli package start"
  },
  "dependencies": {
    "@kozmoai/backend-common": "workspace:^",
    "@kozmoai/backend-plugin-api": "workspace:^",
    "@kozmoai/config": "workspace:^",
    "@kozmoai/errors": "workspace:^",
    "@kozmoai/plugin-auth-node": "workspace:^",
    "@kozmoai/plugin-permission-common": "workspace:^",
    "@types/express": "^4.17.6",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.20.4"
  },
  "devDependencies": {
    "@kozmoai/backend-test-utils": "workspace:^",
    "@kozmoai/cli": "workspace:^",
    "@types/supertest": "^2.0.8",
    "msw": "^1.0.0",
    "supertest": "^6.1.3"
  },
  "files": [
    "dist"
  ]
}
