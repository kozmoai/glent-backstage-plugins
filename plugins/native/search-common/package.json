{
  "name": "@kozmoai/plugin-search-common",
  "version": "1.2.11-next.1",
  "description": "Common functionalities for Search, to be shared between various search-enabled plugins",
  "glint": {
    "role": "common-library"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs.js",
    "types": "dist/index.d.ts"
  },
  "keywords": [
    "glint",
    "search"
  ],
  "homepage": "https://glint.io",
  "bugs": {
    "url": "https://github.com/kozmoai/glint/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kozmoai/glint",
    "directory": "plugins/search-common"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "glint-cli package build",
    "clean": "glint-cli package clean",
