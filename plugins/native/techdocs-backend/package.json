{
  "name": "@kozmoai/plugin-techdocs-backend",
  "version": "1.9.7-next.1",
  "description": "The Glint backend plugin that renders technical documentation for your components",
  "glint": {
    "role": "backend-plugin"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "glint",
    "techdocs"
  ],
  "homepage": "https://glint.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/kozmoai/glint",
    "directory": "plugins/techdocs-backend"
  },
  "license": "Apache-2.0",
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha.ts",
    "./package.json": "./package.json"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "files": [
    "dist",
    "config.d.ts"
  ],
  "scripts": {
    "build": "glint-cli package build",
    "clean": "glint-cli package clean",
    "lint": "glint-cli package lint",
    "prepack": "glint-cli package prepack",
    "postpack": "glint-cli package postpack",
    "start": "glint-cli package start",
    "test": "glint-cli package test"
  },
  "dependencies": {
    "@kozmoai/backend-common": "workspace:^",
    "@kozmoai/backend-plugin-api": "workspace:^",
    "@kozmoai/catalog-client": "workspace:^",
    "@kozmoai/catalog-model": "workspace:^",
    "@kozmoai/config": "workspace:^",
    "@kozmoai/errors": "workspace:^",
    "@kozmoai/integration": "workspace:^",
    "@kozmoai/plugin-catalog-common": "workspace:^",
    "@kozmoai/plugin-permission-common": "workspace:^",
    "@kozmoai/plugin-search-backend-module-techdocs": "workspace:^",
    "@kozmoai/plugin-techdocs-node": "workspace:^",
    "@types/express": "^4.17.6",
    "dockerode": "^4.0.0",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "fs-extra": "^11.2.0",
    "knex": "^3.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "p-limit": "^3.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@kozmoai/backend-test-utils": "workspace:^",
    "@kozmoai/cli": "workspace:^",
    "@types/dockerode": "^3.3.0",
    "msw": "^1.0.0",
    "supertest": "^6.1.3"
  },
  "configSchema": "config.d.ts"
}
