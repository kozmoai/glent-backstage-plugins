{
  "name": "@kozmoai/plugin-search-backend-node",
  "version": "1.2.18-next.1",
  "description": "A library for Glint backend plugins that want to interact with the search backend plugin",
  "glint": {
    "role": "node-library"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://glint.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/kozmoai/glint",
    "directory": "plugins/search-backend-node"
  },
  "license": "Apache-2.0",
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha.ts",
    "./package.json": "./package.json"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "glint-cli package build",
    "clean": "glint-cli package clean",
    "lint": "glint-cli package lint",
    "prepack": "glint-cli package prepack",
    "postpack": "glint-cli package postpack",
    "start": "glint-cli package start",
    "test": "glint-cli package test"
  },
  "dependencies": {
    "@kozmoai/backend-common": "workspace:^",
    "@kozmoai/backend-plugin-api": "workspace:^",
    "@kozmoai/backend-tasks": "workspace:^",
    "@kozmoai/config": "workspace:^",
    "@kozmoai/errors": "workspace:^",
    "@kozmoai/plugin-permission-common": "workspace:^",
    "@kozmoai/plugin-search-common": "workspace:^",
    "@types/lunr": "^2.3.3",
    "lodash": "^4.17.21",
    "lunr": "^2.3.9",
    "ndjson": "^2.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@kozmoai/backend-common": "workspace:^",
    "@kozmoai/cli": "workspace:^",
    "@types/ndjson": "^2.0.1"
  }
}
