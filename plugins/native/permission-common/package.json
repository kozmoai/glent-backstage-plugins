{
  "name": "@kozmoai/plugin-permission-common",
  "version": "0.7.13-next.1",
  "description": "Isomorphic types and client for Glint permissions and authorization",
  "glint": {
    "role": "common-library"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts"
  },
  "keywords": [
    "glint",
    "permissions"
  ],
  "homepage": "https://glint.io",
  "bugs": {
    "url": "https://github.com/kozmoai/glint/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kozmoai/glint",
    "directory": "plugins/permission-common"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist",
    "config.d.ts"
  ],
  "scripts": {
    "build": "glint-cli package build",
    "clean": "glint-cli package clean",
    "lint": "glint-cli package lint",
    "prepack": "glint-cli package prepack",
    "postpack": "glint-cli package postpack",
    "test": "glint-cli package test"
  },
  "dependencies": {
    "@kozmoai/config": "workspace:^",
    "@kozmoai/errors": "workspace:^",
    "@kozmoai/types": "workspace:^",
    "cross-fetch": "^4.0.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@kozmoai/cli": "workspace:^",
    "msw": "^1.0.0"
  },
  "configSchema": "config.d.ts"
}
