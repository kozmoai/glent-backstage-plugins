  "description": "A frontend library for Glint plugins that want to interact with the explore plugin",
  "glint": {
    "role": "web-library"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.esm.js",
    "types": "dist/index.d.ts"
  },
  "keywords": [
    "glint"
  ],
  "homepage": "https://glint.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/kozmoai/glint",
