{
  "name": "@kozmoai/plugin-events-node",
  "description": "The plugin-events-node module for @kozmoai/plugin-events-backend",
  "version": "0.3.0-next.1",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "glint": {
    "role": "node-library"
  },
  "homepage": "https://glint.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/kozmoai/glint",
    "directory": "plugins/events-node"
  },
  "scripts": {
    "start": "glint-cli package start",
    "build": "glint-cli package build",
    "lint": "glint-cli package lint",
    "test": "glint-cli package test",
    "clean": "glint-cli package clean",
    "prepack": "glint-cli package prepack",
    "postpack": "glint-cli package postpack"
  },
  "dependencies": {
    "@kozmoai/backend-plugin-api": "workspace:^"
  },
  "devDependencies": {
    "@kozmoai/backend-common": "workspace:^",
    "@kozmoai/cli": "workspace:^"
  },
  "files": [
    "dist"
  ]
}
