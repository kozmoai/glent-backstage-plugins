    "type": "git",
    "url": "https://github.com/kozmoai/glint",
    "directory": "plugins/search-backend-module-pg"
  },
  "license": "Apache-2.0",
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha.ts",
    "./package.json": "./package.json"
