{
  "name": "@kozmoai/plugin-search-backend-module-pg",
  "version": "0.5.23-next.1",
  "description": "A module for the search backend that implements search using PostgreSQL",
  "glint": {
    "role": "backend-plugin-module"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://glint.io",
