{
  "name": "@kozmoai/plugin-catalog-node",
  "version": "1.8.0-next.1",
  "description": "The plugin-catalog-node module for @kozmoai/plugin-catalog-backend",
  "glint": {
    "role": "node-library"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://glint.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/kozmoai/glint",
    "directory": "plugins/catalog-node"
  },
  "license": "Apache-2.0",
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha.ts",
    "./package.json": "./package.json"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "glint-cli package build",
    "clean": "glint-cli package clean",
    "lint": "glint-cli package lint",
    "prepack": "glint-cli package prepack",
    "postpack": "glint-cli package postpack",
    "start": "glint-cli package start",
    "test": "glint-cli package test"
  },
  "dependencies": {
    "@kozmoai/backend-plugin-api": "workspace:^",
    "@kozmoai/catalog-client": "workspace:^",
    "@kozmoai/catalog-model": "workspace:^",
    "@kozmoai/errors": "workspace:^",
    "@kozmoai/plugin-catalog-common": "workspace:^",
    "@kozmoai/plugin-permission-common": "workspace:^",
    "@kozmoai/plugin-permission-node": "workspace:^",
    "@kozmoai/types": "workspace:^"
  },
  "devDependencies": {
    "@kozmoai/backend-test-utils": "workspace:^",
    "@kozmoai/cli": "workspace:^"
  }
}
