{
  "name": "@kozmoai/plugin-catalog-backend",
  "version": "1.18.0-next.1",
  "description": "The Glint backend plugin that provides the Glint catalog",
  "glint": {
    "role": "backend-plugin"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "glint"
  ],
  "homepage": "https://glint.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/kozmoai/glint",
    "directory": "plugins/catalog-backend"
  },
  "license": "Apache-2.0",
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha.ts",
    "./package.json": "./package.json"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "files": [
    "dist",
    "migrations/**/*.{js,d.ts}",
    "config.d.ts"
  ],
  "scripts": {
    "build": "glint-cli package build",
    "clean": "glint-cli package clean",
    "generate": "glint-repo-tools package schema openapi generate --server --client-package packages/catalog-client",
    "lint": "glint-cli package lint",
    "prepack": "glint-cli package prepack",
    "postpack": "glint-cli package postpack",
    "start": "glint-cli package start",
    "test": "glint-cli package test"
  },
  "dependencies": {
    "@kozmoai/backend-common": "workspace:^",
    "@kozmoai/backend-openapi-utils": "workspace:^",
    "@kozmoai/backend-plugin-api": "workspace:^",
    "@kozmoai/backend-tasks": "workspace:^",
    "@kozmoai/catalog-client": "workspace:^",
    "@kozmoai/catalog-model": "workspace:^",
    "@kozmoai/config": "workspace:^",
    "@kozmoai/errors": "workspace:^",
    "@kozmoai/integration": "workspace:^",
    "@kozmoai/plugin-catalog-common": "workspace:^",
    "@kozmoai/plugin-catalog-node": "workspace:^",
    "@kozmoai/plugin-events-node": "workspace:^",
    "@kozmoai/plugin-permission-common": "workspace:^",
    "@kozmoai/plugin-permission-node": "workspace:^",
    "@kozmoai/plugin-search-backend-module-catalog": "workspace:^",
    "@kozmoai/types": "workspace:^",
    "@opentelemetry/api": "^1.3.0",
    "@types/express": "^4.17.6",
    "codeowners-utils": "^1.0.2",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "fast-json-stable-stringify": "^2.1.0",
    "fs-extra": "^11.2.0",
    "git-url-parse": "^14.0.0",
    "glob": "^7.1.6",
    "knex": "^3.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.0.0",
    "minimatch": "^9.0.0",
    "node-fetch": "^2.6.7",
    "p-limit": "^3.0.2",
    "prom-client": "^15.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.2.1",
    "yaml": "^2.0.0",
    "yn": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@kozmoai/backend-test-utils": "workspace:^",
    "@kozmoai/cli": "workspace:^",
    "@kozmoai/plugin-permission-common": "workspace:^",
    "@kozmoai/repo-tools": "workspace:^",
    "@types/core-js": "^2.5.4",
    "@types/git-url-parse": "^9.0.0",
    "@types/glob": "^8.0.0",
    "@types/lodash": "^4.14.151",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^9.0.0",
    "better-sqlite3": "^9.0.0",
    "luxon": "^3.0.0",
    "msw": "^1.0.0",
    "supertest": "^6.1.3",
    "wait-for-expect": "^3.0.2"
  },
  "configSchema": "config.d.ts"
}
