{
  "name": "@kozmoai/plugin-search-backend",
  "description": "The Glint backend plugin that provides your glint app with search",
  "version": "1.5.4-next.1",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "glint": {
    "role": "backend-plugin"
  },
  "homepage": "https://glint.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/kozmoai/glint",
    "directory": "plugins/search-backend"
  },
  "scripts": {
    "start": "glint-cli package start",
    "build": "glint-cli package build",
    "lint": "glint-cli package lint",
    "test": "glint-cli package test",
    "prepack": "glint-cli package prepack",
    "postpack": "glint-cli package postpack",
    "clean": "glint-cli package clean",
    "generate": "glint-repo-tools package schema openapi generate --server"
  },
  "dependencies": {
    "@kozmoai/backend-common": "workspace:^",
    "@kozmoai/backend-openapi-utils": "workspace:^",
    "@kozmoai/backend-plugin-api": "workspace:^",
    "@kozmoai/config": "workspace:^",
    "@kozmoai/errors": "workspace:^",
    "@kozmoai/plugin-permission-common": "workspace:^",
    "@kozmoai/plugin-permission-node": "workspace:^",
    "@kozmoai/plugin-search-backend-node": "workspace:^",
    "@kozmoai/plugin-search-common": "workspace:^",
    "@kozmoai/types": "workspace:^",
    "@types/express": "^4.17.6",
    "dataloader": "^2.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "qs": "^6.10.1",
    "winston": "^3.2.1",
    "yn": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@kozmoai/backend-test-utils": "workspace:^",
    "@kozmoai/cli": "workspace:^",
    "@types/supertest": "^2.0.8",
    "supertest": "^6.1.3"
  },
  "files": [
    "dist",
    "config.d.ts"
  ],
  "configSchema": "config.d.ts"
}
